{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/upload", "dest": "/index.js" },
    { "src": "/api/videos/:videoId", "dest": "/index.js" },
    { "src": "/api/videos", "dest": "/index.js" },
    { "src": "/api/videos/:videoId", "dest": "/index.js" },
    { "src": "/api/search", "dest": "/index.js" },
    { "src": "/", "dest": "/index.js" }
  ]
}
